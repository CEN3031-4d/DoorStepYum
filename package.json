    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "deploy": "^1.0.3",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "jquery": "^3.4.1",
    "mocha": "^6.2.1",
    "mongoose": "^5.7.1",
    "mongoose-encrypt": "0.0.1-alpha.3",
    "mongoose-encryption": "^2.0.2",
    "morgan": "^1.9.1",
